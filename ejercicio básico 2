import java.util.Scanner;
/* 
 * Cree un programa que demuestre si el numero es multiplo de 10 
 */  
public class EjercicioBasico2 {

	public static void main(String[] args) {
		Scanner entrada=new Scanner(System.in);
		System.out.print("Ingrese un numero:");
		int numero=entrada.nextInt();
        if(numero%10==0)
           System.out.println("El numero "+numero+" es un multiplo de 10");   
        else
           System.out.println("El numero "+numero+" no es multiplo de 10");
	}
}
